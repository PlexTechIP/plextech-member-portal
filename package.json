{
  "name": "plexfinance",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/preset-env": "7.22.5",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.11.11",
    "@mui/material": "5.13.6",
    "@mui/styles": "5.11.13",
    "@mui/x-date-pickers": "5.0.13",
    "@reduxjs/toolkit": "1.8.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@vercel/analytics": "0.1.11",
    "browser-image-compression": "2.0.0",
    "chalk": "4.1.2",
    "compressorjs": "1.1.1",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dayjs": "1.11.7",
    "fontfaceobserver": "2.3.0",
    "husky": "8.0.1",
    "i18next": "21.9.2",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-scanner": "4.0.0",
    "inquirer": "7.3.3",
    "inquirer-directory": "2.2.0",
    "js-cookie": "3.0.5",
    "jwt-decode": "3.1.2",
    "node-plop": "0.26.3",
    "path": "0.12.7",
    "plop": "2.7.6",
    "prettier": "2.7.1",
    "qrcode.react": "3.1.0",
    "react": "18.2.0",
    "react-animated-checkmark": "1.0.3",
    "react-app-polyfill": "3.0.0",
    "react-beautiful-dnd": "13.1.1",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.18.6",
    "react-is": "18.2.0",
    "react-redux": "7.2.8",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.2.1",
    "rimraf": "3.0.2",
    "sanitize.css": "13.0.0",
    "serve": "14.0.1",
    "shelljs": "0.8.5",
    "styled-components": "5.3.5",
    "ts-node": "10.9.1",
    "typescript": "4.6.4",
    "uuid": "9.0.0",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "start:prod": "yarn run build && serve -s build",
    "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "prepare": "husky install"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.x"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^utils/loadable$": "<rootDir>/src/utils/loadable",
      "^utils/useToken$": "<rootDir>/src/utils/useToken",
      "^utils/apiRequest$": "<rootDir>/src/utils/apiRequest",
      "^styles/global-styles$": "<rootDir>/src/styles/global-styles",
      "^images$": "<rootDir>/src/images/imageMock"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!.*\\.(js|jsx|ts|tsx)$)",
      "/images/(?!.*\\.(.*)$)"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/jest": "^27.5.2",
    "@types/js-cookie": "3.0.3",
    "@types/node": "^14.18.27",
    "@types/react": "^18.0.20",
    "@types/react-beautiful-dnd": "13.1.4",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/shelljs": "^0.8.11",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.18.0",
    "babel-jest": "29.5.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jsdom": "22.1.0",
    "lint-staged": "13.0.3",
    "react-test-renderer": "18.2.0",
    "stylelint": "14.12.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "29.1.0"
  }
}
